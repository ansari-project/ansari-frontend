{
  "expo": {
    "name": "Ansari Chat",
    "owner": "ansari-project",
    "slug": "ansari-chat",
    "version": "1.0.0",
    "scheme": "ansarichat",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "icon": "./src/assets/images/icon.png",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      [
        "@sentry/react-native/expo",
        {
          "organization": "ansari-project-llc",
          "project": "ansari-frontend",
          "url": "https://sentry.io/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "supportsRTL": true,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e2f465a7-8007-4e83-91ba-4a9df4c5209a"
      }
    },
    "android": {
      "package": "chat.ansari.app"
    },
    "ios": {
      "bundleIdentifier": "chat.ansari.app",
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e2f465a7-8007-4e83-91ba-4a9df4c5209a"
    }
  }
}
